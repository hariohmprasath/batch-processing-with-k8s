{
  "StartAt": "Single threaded job on EKS",
  "States": {
    "Single threaded job on EKS": {
      "Type": "Task",
      "Resource": "arn:aws:states:::eks:runJob.sync",
      "Parameters": {
        "ClusterName": "BlueGreenDeployeksclusterAE9CBEE2-eb1b3661940a4db8af4f6a1a6ead5e11",
        "CertificateAuthority": "",
        "Endpoint": "https://D57037A4DD71122B48F151510075B842.gr7.us-east-1.eks.amazonaws.com",
        "LogOptions": {
          "RetrieveLogs": false
        },
        "Job": {
          "apiVersion": "batch/v1",
          "kind": "Job",
          "metadata": {
            "generateName": "single-threaded"
          },
          "spec": {
            "backoffLimit": 0,
            "ttlSecondsAfterFinished": 100,
            "template": {
              "metadata": {
                "name": "single-threaded"
              },
              "spec": {
                "containers": [
                  {
                    "name": "single-threaded-container",
                    "image": "775492342640.dkr.ecr.us-east-1.amazonaws.com/single-thread-processor:latest",
                    "env": [
                      {
                        "name": "S3_BUCKET_NAME",
                        "value.$": "$.detail.requestParameters.bucketName"
                      },
                      {
                        "name": "S3_KEY",
                        "value.$": "$.detail.requestParameters.key"
                      },
                      {
                        "name": "STATUS_KEY",
                        "value.$": "$.id"
                      },
                      {
                        "name": "EFS_DIRECTORY",
                        "value": "/data"
                      },
                      {
                        "name": "AWS_REGION",
                        "value": "us-east-1"
                      }
                    ],
                    "volumeMounts": [
                      {
                        "name": "persistent-storage",
                        "mountPath": "/data"
                      }
                    ]
                  }
                ],
                "volumes": [
                  {
                    "name": "persistent-storage",
                    "persistentVolumeClaim": {
                      "claimName": "efs-storage-claim"
                    }
                  }
                ],
                "restartPolicy": "Never"
              }
            }
          }
        }
      },
      "OutputPath": "$.status.succeeded",
      "End": true
    }
  }
}